<!DOCTYPE html>
<html>
  <head>
    <title>Coaching</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <%= render 'layouts/admin' %>

     <style>
           
           header
        {background: -webkit-linear-gradient(to right, #000066, #000066, #000066);
          background: linear-gradient(to right,#000066, #86A8E7, #7F7FD5);
        padding: 20px;
        text-align: right;
        font-size: 16px;
       
        }


        body{color: black;#cc33ff
          background-color: lightcyan}

    

         </style>

  </head>

  <body>

    <%= render 'layouts/header' %> &nbsp;&nbsp;&nbsp;&nbsp;

     <p class="notice" style="color: red; text-align: right;  "><%= notice %></p>
       <p class="alert" style="color: red;"><%= alert %></p>

    <%= yield %>
  </body>
</html>
