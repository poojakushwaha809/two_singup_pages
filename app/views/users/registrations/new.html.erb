<div class="container">
          <div class="middle">
          <h2>Student Sign up</h2>

        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= render "devise/shared/error_messages", resource: resource %>



       <div>
     <%= f.label :student_image %><br />
     <%= f.file_field :student_image %>
</div>

       <div>
     <%= f.label :student_name %><br />
     <%= f.text_field :student_name, autofocus: true %>
</div>
<p></p>
          <div class="field">
            <%= f.label :Student_email %><br>
            <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
          </div>
<p></p>
          <div class="field">
            <%= f.label :password %><br>
            <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
            <% end %><br>
            <%= f.password_field :password, autocomplete: "new-password" %>
          </div>
<p></p>
          <div class="field">
            <%= f.label :password_confirmation %><br>
            <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
          </div>
<p></p>

          <div>
     <%= f.label :student_address %><br />
     <%= f.text_field :student_address, autofocus: true %>
</div>
<p></p>
        <div>
           <%= f.label :Student_contact_number %><br>
            <%= f.text_field :contact_number%>

        </div>
<p></p>

        <div>
           <%= f.label :Student_college_name %><br>
            <%= f.text_field :college_name%>

        </div>

<p></p>

          <div class="actions">
            <%= f.submit "Sign up" %>
            
          </div>
        <% end %>

        <p></p>
        <%= render "users/shared/links" %>

        </div>

</div>
<style type="text/css">
  .middle{text-align: center;}
</style>